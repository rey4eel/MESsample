		    <ValidationRuleType>None</ValidationRuleType>
		    <AllowEdit>True</AllowEdit>
		</Column>
		<Column>
			<Type>CheckedComboBox</Type>
			<Name>Defect</Name>
			<Caption>Defect</Caption>
			<ToolTip><![CDATA[]]></ToolTip>
			<Properties><![CDATA[HorzAlignment=Default]]></Properties>
			<Values><![CDATA[Undefined,PadOverhang,Solderfillet,Coplanarity,Bridging,Dimension,Polarity,OCROCV,Missing,UpsideDown,Foreignmaterial,Absence,WFMI,CDM]]></Values>
